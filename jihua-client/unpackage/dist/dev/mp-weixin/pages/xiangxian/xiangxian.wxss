.slide-top {
		-webkit-animation: slide-top 10s cubic-bezier(0.25, 0.46, 0.45, 0.94) both;
		animation: slide-top 10s cubic-bezier(0.25, 0.46, 0.45, 0.94) both;
}
@-webkit-keyframes slide-top {
0% {
			bottom: -800rpx;
}
100% {
			bottom: 0rpx;
}
}
@keyframes slide-top {
0% {
			bottom: -800rpx;
}
100% {
			bottom: 0rpx;
}
}

